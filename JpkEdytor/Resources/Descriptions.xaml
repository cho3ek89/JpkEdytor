    <s:String x:Key="Ewp1.EwpWiersz.K7">Kwota przychodu opodatkowana wg stawki 8,5%</s:String>
    <s:String x:Key="Ewp1.EwpWiersz.K8">Kwota przychodu opodatkowana wg stawki 5,5%</s:String>
